@{
}

@model IEnumerable<FoodLoverGuide.Models.Category>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var cat in Model)
        {
        <tr>
            <td>@cat.CategoryName</td>
        </tr>
        }
    </tbody>
</table>
